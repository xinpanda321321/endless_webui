{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@webui/application-test": ["application-test/src/index.ts"],
      "@webui/billing-metadata": ["billing-metadata/src/index.ts"],
      "@webui/calendar": ["calendar/src/index.ts"],
      "@webui/candidate-metadata": ["candidate-metadata/src/index.ts"],
      "@webui/client-metadata": ["client-metadata/src/index.ts"],
      "@webui/core": ["core/src/index.ts"],
      "@webui/dashboard": ["dashboard/src/index.ts"],
      "@webui/data": ["data/src/index.ts"],
      "@webui/dialog": ["dialog/src/index.ts"],
      "@webui/dynamic-elements": ["dynamic-elements/src/index.ts"],
      "@webui/dynamic-form": ["dynamic-form/src/index.ts"],
      "@webui/dynamic-list": ["dynamic-list/src/index.ts"],
      "@webui/form-controls": ["form-controls/src/index.ts"],
      "@webui/generic-components": ["generic-components/src/index.ts"],
      "@webui/generic-form": ["generic-form/src/index.ts"],
      "@webui/generic-list": ["generic-list/src/index.ts"],
      "@webui/manager-metadata": ["manager-metadata/src/index.ts"],
      "@webui/metadata": ["metadata/src/index.ts"],
      "@webui/models": ["models/src/index.ts"],
      "@webui/rating": ["rating/src/index.ts"],
      "@webui/shared": ["shared/src/index.ts"],
      "@webui/shared-metadata": ["shared-metadata/src/index.ts"],
      "@webui/stepper": ["stepper/src/index.ts"],
      "@webui/time": ["time/src/index.ts"],
      "@webui/ui": ["ui/src/index.ts"],
      "@webui/utilities": ["utilities/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
