{
  "name": "manager-metadata",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "manager-metadata/src",
  "prefix": "webui",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "manager-metadata/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "manager-metadata/**/*.ts",
          "manager-metadata/**/*.html"
        ]
      }
    }
  }
}
